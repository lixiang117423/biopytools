\
remove_problematic_info() {\
 local input_vcf=$1\
 local output_vcf=$2\
 \
 echo "Ê∏É•ÁÉ∞É¶ÊÉ∂Éß‰ª∂: $input_vcf -> $output_vcf"\
 \
 \
 cp "$input_vcf" "${input_vcf}.backup"\
 \
 \
 awk 'BEGIN{FS=OFS="\t"} \
    /^#/ {print; next}  # Â§¥ÈÉ£®ËÉÅÉ¨ÁÉª¥ÊÉÆ•ËæÉ≥ÂÉß∫\
    {\
        # Â§É§ÁÉ∞É¶INFOÂÉ®É∑ÔºÉ®Á¨¨8ÂÉ®É∑ÔºÉ©\
        info = $8\
        # ÂÉ®ÉÄÈÉπ§ÈÉ∑ÆÈÉÇÉ∏Â≠É∑ÊÆµ\
        gsub(/;?BS_COUNT=[^;]*/, "", info)\
        gsub(/;?GC_COUNT=[^;]*/, "", info)\
        gsub(/;?BS_FREQ=[^;]*/, "", info)\
        gsub(/;?GC_FREQ=[^;]*/, "", info)\
        gsub(/;?LOG2_RATIO=[^;]*/, "", info)\
        # Ê∏É•ÁÉ∞É¶ÂºÄÂ§¥ÁÉ∫É§ÂÉ®É¶ÂÉØ∑\
        gsub(/^;/, "", info)\
        # Â¶ÇÊÉæÉºINFO‰∏∫Á©∫ÔºÉ¨ËÆæ‰∏∫"."\
        if (info == "") info = "."\
        $8 = info\
        print\
    }' "$input_vcf" > "$output_vcf"\
 \
 echo "ÂÆÉ¨ÊÉ®É∞: $output_vcf"\
}\
\
#\
remove_problematic_info "differential_variants.diff.snp.fixed.vcf" "differential_variants.diff.snp.clean.vcf"\
remove_problematic_info "differential_variants.diff.indel.fixed.vcf" "differential_variants.diff.indel.clean.vcf"
