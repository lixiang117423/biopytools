BAM_DIR="/share/org/YZWL/yzwl_lixg/project/17.Â§ßË±É¶ÁÉ∂´ÈÉºÉ©ÂÉø∫ÂÉªÉÄÂÉØÉ∏ÂºÇ_ÊÉΩÉÆÁ£É™/03.mapping/bam"\
\
echo "================================================"\
echo "ÉøÉ¥É≠ Ê£ÄÊÉø•GVCFÊÉ∂Éß‰ª∂‰∏≠ÁÉ∫É§ÊÉÄ∑ÂÉ≥ÅÂÉ∞É≠"\
echo "================================================"\
\
for gvcf in ${VCF_DIR}/*.g.vcf.gz; do\
    filename=$(basename ${gvcf})\
    sample_in_file=$(bcftools query -l ${gvcf})\
    \
    echo ""\
    echo "ÊÉ∂Éß‰ª∂ÂÉ∞É≠: ${filename}"\
    echo "ÊÉ∂Éß‰ª∂ÂÉ¶É•ÊÉÄ∑ÂÉ≥ÅÂÉ∞É≠: ${sample_in_file}"\
    \
    # ÊØÉ¥ËæÉ£ÊÉ∂Éß‰ª∂ÂÉ∞É≠ÂÉ≤É¨ÂÉ¶É•ÈÉ£®ÊÉÄ∑ÂÉ≥ÅÂÉ∞É≠\
    expected_sample=$(basename ${filename} .g.vcf.gz)\
    if [ "${sample_in_file}" != "${expected_sample}" ]; then\
        echo "‚É∫ÉÄÔ∏ÉØ  Ë≠¶ÂÉ±É™: ÊÉÄ∑ÂÉ≥ÅÂÉ∞É≠‰∏É≠ÂÉ¨πÈÉ•É≠\
        echo "   ÊÉºÉøÊÉºÉª: ${expected_sample}"\
        echo "   ÂÆÉæÈÉπÉ•: ${sample_in_file}"\
    else\
        echo "‚ÉºÉ• ÊÉÄ∑ÂÉ≥ÅÂÉ∞É≠Ê≠£ÁÉÅÆ"\
    fi\
done\
\
echo ""\
echo "================================================"\
echo "ÉøÉ¥É≠ Ê£ÄÊÉø•BAMÊÉ∂Éß‰ª∂‰∏≠ÁÉ∫É§Read Group‰øÉÅÊÅØ"\
echo "================================================"\
\
for bam in ${BAM_DIR}/*.sorted.bam; do\
    filename=$(basename ${bam})\
    echo ""\
    echo "ÊÉ∂Éß‰ª∂ÂÉ∞É≠: ${filename}"\
    echo "Read Group‰øÉÅÊÅØ:"\
    samtools view -H ${bam} | grep "^@RG"\
    \
    # ÊÉØÉ∞ÂÉØÉ∂SMÔºÉ®ÊÉÄ∑ÂÉ≥ÅÂÉ∞É≠ÔºÉ©\
    sm=$(samtools view -H ${bam} | grep "^@RG" | sed -n 's/.*\tSM:\([^\t]*\).*/\1/p')\
    echo "ÊÉÄ∑ÂÉ≥ÅÂÉ∞É≠(SM): ${sm}"\
    \
    expected_sample=$(basename ${bam} .sorted.bam)\
    if [ "${sm}" \!= "${expected_sample}" ]; then\
        echo "‚É∫ÉÄÔ∏ÉØ  Ë≠¶ÂÉ±É™: Read GroupÊÉÄ∑ÂÉ≥ÅÂÉ∞É≠‰∏É≠ÂÉ¨πÈÉ•É≠\!"\
        echo "   ÊÉºÉøÊÉºÉª: ${expected_sample}"\
        echo "   ÂÆÉæÈÉπÉ•: ${sm}"\
    else\
        echo "‚ÉºÉ• Read GroupÊÉÄ∑ÂÉ≥ÅÂÉ∞É≠Ê≠£ÁÉÅÆ"\
    fi\
done\
\
