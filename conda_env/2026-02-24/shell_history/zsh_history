#\
#\
echo "INFO: Sorting BAM file by read name. This may take a while..."\
samtools sort -n -@ 16 -m 4G merged_dedup.bam -o merged_dedup.namesorted.bam\
\
#\
#\
echo "INFO: Converting sorted BAM to MND format..."\
samtools view merged_dedup.namesorted.bam | awk 'BEGIN{OFS="\t"} {\
    getline second_line; \
    split(second_line, a, "\t"); \
    str1 = (and($2, 16) ? 1 : 0); \
    str2 = (and(a[2], 16) ? 1 : 0); \
    print $1, $3, $4, str1, a[3], a[4], str2\
}' > merged_nodups.txt\
\
#\
rm merged_dedup.namesorted.bam\
\
echo "INFO: Conversion complete! The required file 'merged_nodups.txt' has been generated."
