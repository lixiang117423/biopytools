echo "# Contig pairs generated from CLM" > sample.filtered.pairs.txt\
awk 'NR>1 {\
    # ÂÉ®É¶Á¶ª contig ÂÉ∞É≠Áß∞ÔºÉ®ÂÉÆªÊÉÆÉ©ÊÉ∂πÂÉ∞É±Á¨¶ÂÉØ∑ +/-ÔºÉ©\
    split($1, a, /[+-]/);\
    split($2, b, /[+-]/);\
    if (a[1] != "" && b[1] != "") {\
        print a[1]"\t"b[1]\
    }\
}' sample.filtered.clm | sort -u >> sample.filtered.pairs.txt\
