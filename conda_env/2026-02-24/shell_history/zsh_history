: 1754643014:0;cat << 'EOF' > /share/org/YZWL/yzwl_lixg/project/95.tetst_meth/03.stat_plot/run_methylkit_analysis.R\
# ===================================================================\
# methylKit Â∑ÆÂºÇÁÉ¥≤ÂÉø∫ÂÉ¨É∂ÂÉ®É¶ÊÉæÉ∞ËÉ§É∫ÊÉº¨ (ÊÉºÄÁªÉ®‰øÆÊ≠£ÁÉ©É®)\
# ===================================================================\
\
# 1. ÂÉ™ÉÄËΩΩÂ∫É≥\
# -------------------------------------------------------------------\
cat("ÂÉ™ÉÄËΩΩ methylKit Â∫É≥...\n")\
library(methylKit)\
\
# 2. ÂÆÉ∫‰πÉ©Ë∑ØÂæÉ§ÂÉ≤É¨ÂÉØÇÊÉµ∞\
# -------------------------------------------------------------------\
cat("ÂÆÉ∫‰πÉ©Ë∑ØÂæÉ§ÂÉ≤É¨ÂÉØÇÊÉµ∞...\n")\
base_dir <- "/share/org/YZWL/yzwl_lixg/project/95.tetst_meth/03.stat_plot"\
sample_info_file <- file.path(base_dir, "sample.txt")\
output_dir <- base_dir\
db_dir <- file.path(output_dir, "methylDB_tabix")\
\
# !!! ÈÉßÉ≠Ë¶ÅÔºÉ∫ËØ∑Â∞É¶ 'your_genome_assembly' ÊÉªøÊÉ≠ÉÇ‰∏∫ÊÇ®ÁÉ∫É§ÂÉØÇËÄÉ£ÂÉø∫ÂÉªÉÄÁªÉ§ÁÉ©É®ÊÉº¨, ‰æÉ´Â¶Ç "hg38", "mm10"Á≠É©\
ref_genome_assembly <- "your_genome_assembly" \
\
dir.create(output_dir, showWarnings = FALSE, recursive = TRUE)\
dir.create(db_dir, showWarnings = FALSE, recursive = TRUE)\
\
# 3. ËØªÂÉØÉ∂ÊÉÄ∑ÊÉº¨ÂÉ•É£ÊÉµ∞ÊÉ≠Æ\
# -------------------------------------------------------------------\
cat("ËØªÂÉØÉ∂ÊÉÄ∑ÊÉº¨ÂÉ•É£ÊÉµ∞ÊÉ≠ÆÊÉ∂Éß‰ª∂...\n")\
sample_info <- read.csv(sample_info_file, header = TRUE, stringsAsFactors = FALSE)\
treatment_vector <- as.integer(factor(sample_info$treatment_group, levels = c("CK", "Treatment"))) - 1\
\
# 4. ËØªÂÉØÉ∂ Bismark CpG ÊÉ™•ÂÉ±É™ÊÉ∂Éß‰ª∂\
# -------------------------------------------------------------------\
cat("‰ΩøÁÉ¥® methRead ËØªÂÉØÉ∂ Bismark ÊÉ∂Éß‰ª∂ (Â¶ÇÊÉæÉºÊÉ∂Éß‰ª∂ÂæÉ®Â§ßÔºÉ¨ËøÉπÂ∞É¶ÈÉºÄË¶Å‰∏Ä‰∫ÉªÊÉ∑∂ÈÉ∑¥)...\n")\
my_meth_raw <- methRead(\
  location = as.list(sample_info$filepath),\
  sample.id = as.list(sample_info$sample_id),\
  assembly = ref_genome_assembly,\
  treatment = treatment_vector,\
  context = "CpG",\
  # -------------------- ÂÉ•≥ÈÉ¥Æ‰øÆÂ§É≠ --------------------\
  # ÊÉ∏ÉÆÁÉÅÆÂÉ±É™ÁÉø• methRead ÊÉ∂Éß‰ª∂ÊÉΩ•ËÉß™‰∫ÉÆ Bismark CpG ÊÉ™•ÂÉ±É™ÊµÅÁ®É´\
  pipeline = "bismarkCpG",\
  # --------------------------------------------------\
  dbtype = "tabix",\
  dbdir = db_dir\
)\
\
# 5. Ë¥®ÊÉÆß‰∏ÉÆËøÉßÊª§\
# -------------------------------------------------------------------\
cat("ËøÉªËÉÅÉ¨Ë¥®ÈÉßÉØÊÉÆßÂÉ®∂ÂÉ≤É¨ËøÉßÊª§...\n")\
filtered_meth_list <- filterByCoverage(\
  my_meth_raw,\
  lo.count = 10,\
  lo.perc = NULL,\
  hi.count = NULL,\
  hi.perc = 99.9\
)\
\
# 6. ÂÉ∞É®Âπ∂ÊÉ©ÄÊÉºÉ©ÊÉÄ∑ÊÉº¨\
# -------------------------------------------------------------------\
cat("ÂÉ∞É®Âπ∂ÊÉ©ÄÊÉºÉ©ÊÉÄ∑ÊÉº¨‰ª•ËøÉªËÉÅÉ¨ÊØÉ¥ËæÉ£...\n")\
meth_all <- unite(filtered_meth_list, destrand = FALSE, normalize = TRUE) \
\
# 7. ÊÉÄ∑ÊÉº¨ÁÉª∏ÂÉ•≥ÊÄßÂÉ®É¶ÊÉæÉ∞\
# -------------------------------------------------------------------\
cat("ËÆÉÅÁÆÉ∑ÊÉÄ∑ÊÉº¨ÁÉª∏ÂÉ•≥ÊÄßÂπ∂ÁªÉ∏ÂÉªæ...\n")\
sample_correlation <- getCorrelation(meth_all, plot = FALSE)\
write.csv(sample_correlation, file.path(output_dir, "sample_correlation_matrix.csv"))\
\
pdf(file.path(output_dir, "sample_correlation_heatmap.pdf"), width = 8, height = 8)\
getCorrelation(meth_all, plot = TRUE)\
dev.off()\
\
pdf(file.path(output_dir, "sample_clustering_dendrogram.pdf"))\
clusterSamples(meth_all, dist = "correlation", method = "ward.D2", plot = TRUE)\
dev.off()\
\
pdf(file.path(output_dir, "sample_PCA_plot.pdf"))\
PCASamples(meth_all, screeplot = TRUE)\
PCASamples(meth_all)\
dev.off()\
\
\
# 8. ËÆÉÅÁÆÉ∑Â∑ÆÂºÇÁÉ¥≤ÂÉø∫ÂÉ¨É∂\
# -------------------------------------------------------------------\
cat("ËÆÉÅÁÆÉ∑Â∑ÆÂºÇÁÉ¥≤ÂÉø∫ÂÉ¨É∂‰ΩÉ≠ÁÇπ (DMCs)...\n")\
my_diff <- calculateDiffMeth(meth_all, mc.cores = 8)\
\
# 9. ÊÉØÉ∞ÂÉØÉ∂Âπ∂‰øÉΩÂ≠É∏Â∑ÆÂºÇÁÉ¥≤ÂÉø∫ÂÉ¨É∂ÁªÉ≥ÊÉæÉº\
# -------------------------------------------------------------------\
cat("ÊÉØÉ∞ÂÉØÉ∂Âπ∂‰øÉΩÂ≠É∏Â∑ÆÂºÇÁÉ¥≤ÂÉø∫ÂÉ¨É∂ÁªÉ≥ÊÉæÉº...\n")\
diff_meth_all <- getData(my_diff)\
write.csv(diff_meth_all, file = file.path(output_dir, "differential_methylation_all_sites.csv"), row.names = FALSE)\
\
diff_meth_significant <- getMethylDiff(my_diff, difference = 25, qvalue = 0.01)\
write.csv(diff_meth_significant, file = file.path(output_dir, "differential_methylation_significant_sites_diff25_q0.01.csv"), row.names = FALSE)\
\
diff_meth_hyper <- getMethylDiff(my_diff, difference = 25, qvalue = 0.01, type = "hyper")\
write.csv(diff_meth_hyper, file = file.path(output_dir, "DMC_hypermethylated_in_Treatment.csv"), row.names = FALSE)\
\
diff_meth_hypo <- getMethylDiff(my_diff, difference = 25, qvalue = 0.01, type = "hypo")\
write.csv(diff_meth_hypo, file = file.path(output_dir, "DMC_hypomethylated_in_Treatment.csv"), row.names = FALSE)\
\
# 10. ÂÉØØËßÉ¶ÂÉ¨É∂Â∑ÆÂºÇÁÉ¥≤ÂÉø∫ÂÉ¨É∂ÁªÉ≥ÊÉæÉº\
# -------------------------------------------------------------------\
cat("ÂÉØØËßÉ¶ÂÉ¨É∂Â∑ÆÂºÇÁÉ¥≤ÂÉø∫ÂÉ¨É∂ÁªÉ≥ÊÉæÉº...\n")\
pdf(file.path(output_dir, "differential_methylation_by_chromosome.pdf"), width = 10, height = 6)\
diffMethPerChr(my_diff, plot = TRUE, qvalue.cutoff = 0.01, meth.cutoff = 25)\
dev.off()\
\
\
cat("\nÂÉ®É¶ÊÉæÉ∞ÂÆÉ¨ÊÉ®É∞ÔºÅÊÉ©ÄÊÉºÉ©ÁªÉ≥ÊÉæÉºÂ∑≤ËæÉ≥ÂÉß∫ÂÉ®∞ÁÉªÆÂΩÉµÔºÉ∫", output_dir, "\n")\
q()\
EOF
