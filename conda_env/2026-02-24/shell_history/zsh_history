: 1754643014:0;# ÂÉ®ÉªÂª∫ÊÉµ∞ÊÉ≠ÆÁÉ¥ÉøÊÉ®É∞ËÉ§É∫ÊÉº¨\
cat > scripts/create_test_data.py << 'EOF'\
#!/usr/bin/env python3\
"""ÂÉ®ÉªÂª∫Ê≥ÉªÂÉø∫ÂÉªÉÄÁªÉ§ÊÉæÉ§Âª∫ÊµÉ´ËØÉµÊÉµ∞ÊÉ≠Æ"""\
import os\
import random\
from pathlib import Path\
from Bio.Seq import Seq\
from Bio.SeqRecord import SeqRecord\
from Bio import SeqIO\
\
def generate_chromosome(name, length=30000):\
    """ÁÉ¥ÉøÊÉ®É∞ÈÉ∫ÉØÊÉº∫ÊÉøÉ≥ËÉ©≤‰ΩÉ≥Â∫ÉØÂÉ®É∑"""\
    bases = ['A', 'T', 'G', 'C']\
    sequence = ''.join(random.choices(bases, k=length))\
    return SeqRecord(Seq(sequence), id=name, description=f"Test chromosome {name}")\
\
def create_variant_genome(original_file, output_file, num_variants=3):\
    """ÂÉ®ÉªÂª∫Â∏¶ÂÉØÉ∏ÂºÇÁÉ∫É§ÂÉø∫ÂÉªÉÄÁªÉ§"""\
    records = list(SeqIO.parse(original_file, "fasta"))\
    modified_records = []\
    \
    for record in records:\
        seq = str(record.seq)\
        \
        # ÂÉº®ÈÉ∫ÉØÊÉº∫‰ΩÉ≠ÁΩÆÊÉØÉ≤ÂÉ••Â∫ÉØÂÉ®É∑ (Ê®ÉÅÊÉ´ÉøPAV)\
        if len(seq) > 2000:\
            positions = sorted(random.sample(range(1000, len(seq)-1000), num_variants))\
            \
            new_seq = seq\
            offset = 0\
            for pos in positions:\
                # ÊÉØÉ≤ÂÉ••ÈÉ∫ÉØÊÉº∫Â∫ÉØÂÉ®É∑ (50-200bp)\
                insert_len = random.randint(50, 200)\
                insert_seq = ''.join(random.choices(['A', 'T', 'G', 'C'], k=insert_len))\
                new_seq = new_seq[:pos+offset] + insert_seq + new_seq[pos+offset:]\
                offset += insert_len\
        else:\
            new_seq = seq\
        \
        modified_record = SeqRecord(\
            Seq(new_seq), \
            id=record.id, \
            description=f"Modified genome with variants"\
        )\
        modified_records.append(modified_record)\
    \
    SeqIO.write(modified_records, output_file, "fasta")\
    return len(modified_records)\
\
def create_gff_annotation(fasta_file, gff_file):\
    """ÂÉ®ÉªÂª∫ÁÆÄÂÉ≠ÉµÁÉ∫É§GFFÊ≥®ÈÉßÉ™ÊÉ∂Éß‰ª∂"""\
    records = list(SeqIO.parse(fasta_file, "fasta"))\
    \
    with open(gff_file, 'w') as f:\
        f.write("##gff-version 3\n")\
        \
        gene_id = 1\
        for record in records:\
            chr_name = record.id\
            chr_len = len(record.seq)\
            \
            # ÊØÉØ2000bpÊÉ¥æÁΩÆ‰∏Ä‰∏™ÂÉø∫ÂÉªÉÄ\
            for pos in range(500, chr_len-500, 2000):\
                gene_start = pos\
                gene_end = min(pos + 1000, chr_len-100)\
                \
                if gene_end - gene_start < 300:\
                    continue\
                \
                # ÂÉø∫ÂÉªÉÄÁÉ©πÂæÅ\
                f.write(f"{chr_name}\ttest\tgene\t{gene_start}\t{gene_end}\t.\t+\t.\t")\
                f.write(f"ID=gene{gene_id:06d};Name=TEST{gene_id:06d}\n")\
                \
                f.write(f"{chr_name}\ttest\tmRNA\t{gene_start}\t{gene_end}\t.\t+\t.\t")\
                f.write(f"ID=mRNA{gene_id:06d};Parent=gene{gene_id:06d}\n")\
                \
                f.write(f"{chr_name}\ttest\texon\t{gene_start}\t{gene_end}\t.\t+\t.\t")\
                f.write(f"ID=exon{gene_id:06d};Parent=mRNA{gene_id:06d}\n")\
                \
                cds_start = gene_start + 50\
                cds_end = gene_end - 50\
                f.write(f"{chr_name}\ttest\tCDS\t{cds_start}\t{cds_end}\t.\t+\t0\t")\
                f.write(f"ID=cds{gene_id:06d};Parent=mRNA{gene_id:06d}\n")\
                \
                gene_id += 1\
    \
    return gene_id - 1\
\
def main():\
    """‰∏ªÂÉßΩÊÉµ∞"""\
    print("Éøß¨ ÂÉ®ÉªÂª∫Ê≥ÉªÂÉø∫ÂÉªÉÄÁªÉ§ÊµÉ´ËØÉµÊÉµ∞ÊÉ≠Æ...")\
    \
    # ÁÉÅÆ‰øÉΩÁÉªÆÂΩÉµÂ≠É∏ÂÉº®\
    Path("genomes").mkdir(exist_ok=True)\
    \
    # ÂÉ®ÉªÂª∫ÂÉØÇËÄÉ£ÂÉø∫ÂÉªÉÄÁªÉ§ (3‰∏™ÊÉøÉ≥ËÉ©≤‰ΩÉ≥)\
    print("ÂÉ®ÉªÂª∫ÂÉØÇËÄÉ£ÂÉø∫ÂÉªÉÄÁªÉ§...")\
    ref_records = [\
        generate_chromosome("Chr01", 25000),\
        generate_chromosome("Chr02", 20000),\
        generate_chromosome("Chr03", 15000)\
    ]\
    SeqIO.write(ref_records, "genomes/reference.fa", "fasta")\
    num_genes = create_gff_annotation("genomes/reference.fa", "genomes/reference.gff")\
    print(f"  ÂÉØÇËÄÉ£ÂÉø∫ÂÉªÉÄÁªÉ§: 3ÊÉΩÉÅÊÉøÉ≥ËÉ©≤‰ΩÉ≥, {num_genes}‰∏™ÂÉø∫ÂÉªÉÄ")\
    \
    # ÂÉ®ÉªÂª∫ÊÉø•ËØÉÇÂÉø∫ÂÉªÉÄÁªÉ§\
    genome_names = ["genome1", "genome2", "genome3"]\
    for name in genome_names:\
        print(f"ÂÉ®ÉªÂª∫ {name} ÂÉø∫ÂÉªÉÄÁªÉ§...")\
        create_variant_genome("genomes/reference.fa", f"genomes/{name}.fa", num_variants=2)\
        num_genes = create_gff_annotation(f"genomes/{name}.fa", f"genomes/{name}.gff")\
        print(f"  {name}: {num_genes}‰∏™ÂÉø∫ÂÉªÉÄ")\
    \
    # ÂÉ®ÉªÂª∫ÂÉø∫ÂÉªÉÄÁªÉ§ÂÉ®É∑ËÉÅ®ÊÉ∂Éß‰ª∂\
    with open("genome_list.txt", "w") as f:\
        f.write("reference.fa\n")\
        for name in genome_names:\
            f.write(f"{name}.fa\n")\
    \
    print("‚ÉºÉ• ÊµÉ´ËØÉµÊÉµ∞ÊÉ≠ÆÂÉ®ÉªÂª∫ÂÆÉ¨ÊÉ®É∞!")\
    print(f"   ÂÉø∫ÂÉªÉÄÁªÉ§ÊÉµ∞ÈÉßÉØ: {len(genome_names) + 1}")\
    \
    # ÊÉ∏æÁ§∫ÊÉ∂Éß‰ª∂‰øÉÅÊÅØ\
    print("\nÉøÉ≥Å ÂÉ®ÉªÂª∫ÁÉ∫É§ÊÉ∂Éß‰ª∂:")\
    os.system("ls -la genomes/")\
\
if __name__ == "__main__":\
    main()\
EOF\
\
