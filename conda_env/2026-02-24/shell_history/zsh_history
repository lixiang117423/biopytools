: 1762418962:0;cat << 'EOF' > plot_snp_density.sh\
#!/bin/bash\
set -e\
\
# --- 1. ÁÉ¥®ÊÉ®∑ÈÉ•É≠ÁΩÆÂÉ¨∫ ---\
VCF_FILE="variation.filtered.snp.vcf.gz"\
WINDOW_SIZE=1000000 \
OUTPUT_PREFIX="snp_density"\
\
echo "=== Ê≠•È™§ 1: ÂÉ®ÉªÂª∫ÊÉøÉ≥ËÉ©≤‰ΩÉ≥ÂÉ∞É≠Áß∞ÊÉ∏ÉÄÂ∞É§ÊÉ∂Éß‰ª∂ ==="\
cat << EOM > chr.map\
NC_081805.1  Chr1\
NC_081806.1  Chr2\
NC_081807.1  Chr3\
NC_081808.1  Chr4\
NC_081809.1  Chr5\
NC_081810.1  Chr6\
NC_081811.1  Chr7\
NC_081812.1  Chr8\
NC_081813.1  Chr9\
NC_081814.1 Chr10\
NC_081815.1 Chr11\
NC_081816.1 Chr12\
NC_081817.1 Chr13\
NC_081818.1 Chr14\
NC_081819.1 Chr15\
NC_081820.1 Chr16\
NC_081821.1 Chr17\
NC_081822.1 Chr18\
NC_081823.1 Chr19\
EOM\
echo "ÊÉ∂Éß‰ª∂ 'chr.map' ÂÉ®ÉªÂª∫ÊÉ®É∞ÂÉ™Éø."\
\
echo -e "\n=== Ê≠•È™§ 2: ÁÉ¥ÉøÊÉ®É∞ÊÉøÉ≥ËÉ©≤‰ΩÉ≥ÈÉµøÂ∫¶ÊÉ∂Éß‰ª∂ (ÊÉΩ•ËÉß™VCFÂ§¥ÊÉ∂Éß‰ª∂) ==="\
gunzip -c ${VCF_FILE} | grep '^##contig' | sed -e 's/.*<ID=//' -e 's/,length=/\t/' -e 's/>.*//' | \\
awk 'FNR==NR{map[\$1]=\$2; next} {if (\$1 in map) print map[\$1] "\t" \$2}' chr.map - > genome.len\
echo "ÊÉ∂Éß‰ª∂ 'genome.len' ÂÉ®ÉªÂª∫ÊÉ®É∞ÂÉ™Éø."\
\
echo -e "\n=== Ê≠•È™§ 3: ‰ªÉÆ VCF ÊÉ∂Éß‰ª∂‰∏≠ÊÉØÉ∞ÂÉØÉ∂ SNP ‰ΩÉ≠ÁΩÆÂπ∂ËΩ¨ÊÉ≠ÉÇ‰∏∫ BED ÊÉÄºÂºÉØ ==="\
gunzip -c ${VCF_FILE} | grep -v '^#' | awk -v OFS='\t' '{print \$1, \$2-1, \$2}' | \\
awk 'FNR==NR{map[\$1]=\$2; next} {if (\$1 in map) {\$1=map[\$1]; print}}' chr.map - > ${OUTPUT_PREFIX}.positions.bed\
echo "ÊÉ∂Éß‰ª∂ '${OUTPUT_PREFIX}.positions.bed' ÂÉ®ÉªÂª∫ÊÉ®É∞ÂÉ™Éø."\
\
echo -e "\n=== Ê≠•È™§ 4: ËÆÉÅÁÆÉ∑ÊÉ¨ÉßÂÆÉ∫Á™É∑ÂÉØ£Â§ßÂ∞ÉØÁÉ∫É§ SNP ÂØÉ¶Â∫¶ (Â∑≤‰øÆÊ≠£‰∏∫ 'bins') ==="\
python -m jcvi.formats.bed bins ${OUTPUT_PREFIX}.positions.bed ${WINDOW_SIZE} > ${OUTPUT_PREFIX}.density.bed\
echo "ÊÉ∂Éß‰ª∂ '${OUTPUT_PREFIX}.density.bed' ÂÉ®ÉªÂª∫ÊÉ®É∞ÂÉ™Éø."\
\
echo -e "\n=== Ê≠•È™§ 5: ÂÉ®ÉªÂª∫ JCVI ÁªÉ∏ÂÉªæÂ∏É£Â±ÄÊÉ∂Éß‰ª∂ ==="\
cat << EOM > ${OUTPUT_PREFIX}.layout\
# y, x, rotation, height, width\
.canvas\
8, 5, 0, 1200, 600\
# seqid, length\
.seqids\
\$(cat genome.len)\
# trackname, color, min, max, height, file\
.tracks\
heatmap, viridis, 0, db, 80, ${OUTPUT_PREFIX}.density.bed\
# options\
.config\
proportional=no\
spacing=20\
EOM\
echo "ÊÉ∂Éß‰ª∂ '${OUTPUT_PREFIX}.layout' ÂÉ®ÉªÂª∫ÊÉ®É∞ÂÉ™Éø."\
\
echo -e "\n=== Ê≠•È™§ 6: ‰ΩøÁÉ¥® JCVI ÁªÉ∏ÂÉ®∂ÊÉøÉ≥ËÉ©≤‰ΩÉ≥ÂØÉ¶Â∫¶ÂÉªæ ==="\
python -m jcvi.graphics.karyotype ${OUTPUT_PREFIX}.layout\
echo -e "\nÉøÉÆÉ© ÁªÉ∏ÂÉªæÂÆÉ¨ÊÉ®É∞! ËæÉ≥ÂÉß∫ÊÉ∂Éß‰ª∂‰∏∫ 'karyotype.pdf' ÂÉ≤É¨ 'karyotype.png'."\
\
EOF
