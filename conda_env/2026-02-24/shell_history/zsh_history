    n = split($9, attrs, ";");\
    new_attrs = "";\
    for (i = 1; i <= n; i++) {\
        if (attrs[i] ~ /^ID=/ || attrs[i] ~ /^Name=/ || attrs[i] ~ /^Parent=/) {\
            if (new_attrs == "") {\
                new_attrs = attrs[i];\
            } else {\
                new_attrs = new_attrs ";" attrs[i];\
            }\
        }\
    }\
    $9 = new_attrs;\
    print $0;\
}' genome.gff3 > genome.clean.gff3
