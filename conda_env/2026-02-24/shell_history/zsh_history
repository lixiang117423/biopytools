: 1765172539:0;xx OV53_result.asm.hist.temp
: 1765172657:0;echo ">>> Ê≠£ÂÉº®ËÆÉÅÁÆÉ∑ QV..."\
\
awk -v k=21 '\
BEGIN {\
    total = 0;\
    found = 0;\
}\
{\
    # ÊØÉØ‰∏ÄËÉÅÉ¨‰ª£ËÉÅ®ÂÉø∫ÂÉªÉÄÁªÉ§ÁÉ∫É§‰∏Ä‰∏™ k-mer ÂÉº® reads ÈÉßÉ¨ÁÉ∫É§ËÆÉÅÊÉµ∞ÂÄº\
    total += 1;\
    if ($1 > 0) {\
        found += 1;\
    }\
}\
END {\
    if (total == 0) {\
        print "ÈÉ¥ÉπËØØÔºÉ∫Ê≤ÉÅÊÉºÉ©ËØªÂÉØÉ∂ÂÉ®∞ K-mer ÊÉµ∞ÊÉ≠Æ„ÄÇ";\
        exit;\
    }\
    \
    # ËÆÉÅÁÆÉ∑ k-mer ÁÉ¥ÉøÂ≠É∏ÁÉÆÉß (survival rate)\
    survival = found / total;\
    \
    if (survival == 1) {\
        print "QV ÂÄº: Inf (ÂÆÉ¨ÂÉ•®ÂÉ¨πÈÉ•É≠ÔºÉ¨ÂÉØØËÉ£ΩÊÉ∏ØÂºÇÂ∏∏)";\
    } else if (survival == 0) {\
        print "QV ÂÄº: 0";\
    } else {\
        # Merqury QV ÂÉ•¨ÂºÉØ: -10 * log10(1 - survival^(1/k))\
        # ‰πÉøÂ∞±ÊÉ∏ØÔºÉ∫ÈÉ¥ÉπËØØÁÉÆÉß E = 1 - survival^(1/k)\
        # QV = -10 * log10(E)\
        \
        # awk ÁÉ∫É§ log ÊÉ∏ØËÉß™ÁÉ§∂ÂØπÊÉµ∞ lnÔºÉ¨ÊÉ≠ÉÇÁÆÉ∑ log10(x) = log(x)/log(10)\
        e_rate = 1 - (survival ^ (1/k));\
        qv = -10 * (log(e_rate) / log(10));\
        \
        print "----------------------------------------"\
        printf "ÊÄª K-mers ÊÉµ∞: %d\n", total\
        printf "ÂÉ¨πÈÉ•É≠ K-mers ÊÉµ∞: %d\n", found\
        printf "ÂÉ¨πÈÉ•É≠ÊØÉ¥‰æÉ´: %.6f\n", survival\
        printf ">>> ÊÉºÄÁªÉ® QV ÂÄº: %.2f\n", qv\
        print "----------------------------------------"\
    }\
}' ASM.freq.clean
