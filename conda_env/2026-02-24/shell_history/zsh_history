    --out bsa_50_50.merged.snp.filtered
: 1754643014:0;cd 07.gwas/gtx
: 1754643014:0;rm data_with_pheno.*
: 1754643014:0;rm quick_test.*
: 1754643014:0;m suggestive_hits.txt
: 1754643014:0;rm significant_hits.txt
: 1754643014:0;rm suggestive_hits.txt
: 1754643014:0;rm analysis_report.txt
: 1754643014:0;rm pca_results.log
: 1754643014:0;rm pruned_snps.log
: 1754643014:0;rm related_individuals.txt
: 1754643014:0;vi run_plink_gwas.sh#\
# =============================================================================\
# ÂÆÉ¨ÊÉµ¥ÁÉ∫É§PLINKË¥®ÈÉßÉØÊÄßÁÉ™∂GWASÂÉ®É¶ÊÉæÉ∞ËÉ§É∫ÊÉº¨\
# ÊÉ¥ØÊÉ¨ÅÔºÉ∫ÈÉΩÉæÊÉÄÉßÂÉßÉ¶ÊÉøÉ≥ËÉ©≤‰ΩÉ≥ÁºÉ∂ÂÉØ∑„ÄÅÁº∫Â§±ÊÄßÂÉ®´‰øÉÅÊÅØ„ÄÅÊÉπ∫ËÉ£ΩPÂÄºÊ£ÄÊµÉ´\
# ËæÉ≥ÂÉ••ÊÉ∂Éß‰ª∂ÔºÉ∫final_filtered.recode.chr.vcf.gz, phe.txt\
# ËæÉ≥ÂÉß∫ÔºÉ∫ÂÆÉ¨ÊÉµ¥ÁÉ∫É§GWASÂÉ®É¶ÊÉæÉ∞ÁªÉ≥ÊÉæÉº\
# =============================================================================\
\
echo "================================================"\
echo "ÂÆÉ¨ÊÉµ¥GWASÂÉ®É¶ÊÉæÉ∞ÊµÅÁ®É´ - ÊÉ§ÉøÁÉ∑É•/ÊÉ™É∑ÁÉ∑É•ÊÄßÁÉ™∂"\
echo "ÁÉ©É®ÊÉº¨: ÊÉºÄÁªÉ®ÂÆÉ¨ÊÉµ¥ÁÉ©É®"\
echo "================================================"\
\
# Ê£ÄÊÉø•ËæÉ≥ÂÉ••ÊÉ∂Éß‰ª∂\
if [ ! -f "final_filtered.recode.chr.vcf.gz" ]; then\
    echo "ÈÉ¥ÉπËØØ: ÊÉ©æ‰∏É≠ÂÉ®∞ final_filtered.recode.chr.vcf.gz"\
    exit 1\
fi\
\
if [ ! -f "phe.txt" ]; then\
    echo "ÈÉ¥ÉπËØØ: ÊÉ©æ‰∏É≠ÂÉ®∞ phe.txt"\
    exit 1\
fi\
\
# ÂÉ®ÉªÂª∫Â∑•‰ΩÉºÁÉªÆÂΩÉµ\
mkdir -p gwas_results\
cd gwas_results\
\
# Â§É≠ÂÉ®∂ËæÉ≥ÂÉ••ÊÉ∂Éß‰ª∂\
cp ../final_filtered.recode.chr.vcf.gz .\
cp ../phe.txt .\
\
# =============================================================================\
# Á¨¨‰∏ÄÊ≠•: ËÉÅ®ÂÉæÉ´ÊÉ∂Éß‰ª∂Â§É§ÁÉ∞É¶\
# =============================================================================\
echo ""\
echo "Á¨¨‰∏ÄÊ≠•: ËÉÅ®ÂÉæÉ´ÊÉ∂Éß‰ª∂Â§É§ÁÉ∞É¶..."\
\
# Ê£ÄÊÉø•ÂÉÆÉøÂßÉ´ËÉÅ®ÂÉæÉ´ÊÉ∂Éß‰ª∂\
echo "ÂÉÆÉøÂßÉ´ËÉÅ®ÂÉæÉ´ÊÉ∂Éß‰ª∂ÁªÉøËÆÉÅÔºÉ∫"\
echo "ÊÄªÊÉÄ∑ÊÉº¨ÊÉµ∞: $(tail -n +2 ../phe.txt | wc -l)"\
echo "ÊÉ§ÉøÁÉ∑É•ÊÉÄ∑ÊÉº¨ (DI=1): $(awk 'NR>1 && $2==1' ../phe.txt | wc -l)"\
echo "ÊÉ™É∑ÁÉ∑É•ÊÉÄ∑ÊÉº¨ (DI=0): $(awk 'NR>1 && $2==0' ../phe.txt | wc -l)"\
\
# ËΩ¨ÊÉ≠ÉÇ‰∏∫PLINKÊÉÄºÂºÉØ\
awk 'NR>1 {\
    FID = $1;\
    IID = $1;\
    if ($2 == 0) {\
        phenotype = 1;  # ÊÉ™É∑ÁÉ∑É• -> ÂØπÁÉ•ß\
    } else if ($2 == 1) {\
        phenotype = 2;  # ÊÉ§ÉøÁÉ∑É• -> ÁÉ∑É•‰æÉ´\
    } else {\
        phenotype = -9; # Áº∫Â§±\
    }\
    print FID, IID, phenotype;\
}' phe.txt > phenotype_formatted.txt\
\
echo "PLINKÊÉÄºÂºÉØËÉÅ®ÂÉæÉ´ÁªÉøËÆÉÅÔºÉ∫"\
echo "ÂØπÁÉ•ßÊÉµ∞ (ÊÉ™É∑ÁÉ∑É•): $(awk '$3==1' phenotype_formatted.txt | wc -l)"\
echo "ÁÉ∑É•‰æÉ´ÊÉµ∞ (ÊÉ§ÉøÁÉ∑É•): $(awk '$3==2' phenotype_formatted.txt | wc -l)"\
echo "Áº∫Â§±: $(awk '$3==-9' phenotype_formatted.txt | wc -l)"\
\
# =============================================================================\
# Á¨¨‰∫É¨Ê≠•: VCFÊÉ∂Éß‰ª∂ËΩ¨ÊÉ≠ÉÇ\
# =============================================================================\
echo ""\
echo "Á¨¨‰∫É¨Ê≠•: VCFÊÉ∂Éß‰ª∂ËΩ¨ÊÉ≠ÉÇ..."\
\
plink --vcf final_filtered.recode.chr.vcf.gz \\
      --make-bed \\
      --out raw_data \\
      --allow-extra-chr \\
      --set-missing-var-ids @:# \\
      --keep-allele-order\
\
if [ ! -f "raw_data.bed" ]; then\
    echo "ÈÉ¥ÉπËØØ: VCFËΩ¨ÊÉ≠ÉÇÂ§±Ë¥•"\
    exit 1\
fi\
\
echo "VCFËΩ¨ÊÉ≠ÉÇÊÉ®É∞ÂÉ™Éø"\
echo "ÂÉÆÉøÂßÉ´ÊÉÄ∑ÊÉº¨ÊÉµ∞: $(wc -l < raw_data.fam)"\
echo "ÂÉÆÉøÂßÉ´SNPÊÉµ∞: $(wc -l < raw_data.bim)"\
\
# Ê£ÄÊÉø•ÊÉøÉ≥ËÉ©≤‰ΩÉ≥ÂÉ®É¶Â∏É£\
echo "ÊÉøÉ≥ËÉ©≤‰ΩÉ≥ÂÉ®É¶Â∏É£ÔºÉ®ÂÉ©É≠10‰∏™ÔºÉ©:"\
awk '{print $1}' raw_data.bim | sort | uniq -c | head -10\
\
# =============================================================================\
# Á¨¨‰∏É©Ê≠•: ÂÉ∞É®Âπ∂ËÉÅ®ÂÉæÉ´‰øÉÅÊÅØ\
# =============================================================================\
echo ""\
echo "Á¨¨‰∏É©Ê≠•: ÂÉ∞É®Âπ∂ËÉÅ®ÂÉæÉ´‰øÉÅÊÅØ..."\
\
plink --bfile raw_data \\
      --pheno phenotype_formatted.txt \\
      --make-bed \\
      --allow-extra-chr \\
      --allow-no-sex \\
      --out data_with_pheno\
\
if [ ! -f "data_with_pheno.bed" ]; then\
    echo "ÈÉ¥ÉπËØØ: ËÉÅ®ÂÉæÉ´ÂÉ∞É®Âπ∂Â§±Ë¥•"\
    \
    # ËØÉ™ÊÉ∂≠ÊÉÄ∑ÊÉº¨IDÂÉ¨πÈÉ•É≠ÈÉ∑ÆÈÉÇÉ∏\
    echo "ËØÉ™ÊÉ∂≠ÊÉÄ∑ÊÉº¨IDÂÉ¨πÈÉ•É≠..."\
    awk '{print $2}' raw_data.fam | sort > genotype_samples.tmp\
    awk '{print $2}' phenotype_formatted.txt | sort > phenotype_samples.tmp\
    \
    common=$(comm -12 genotype_samples.tmp phenotype_samples.tmp | wc -l)\
    echo "ÂÉø∫ÂÉªÉÄÂÉæÉ´ÊÉ∂Éß‰ª∂ÊÉÄ∑ÊÉº¨ÊÉµ∞: $(wc -l < genotype_samples.tmp)"\
    echo "ËÉÅ®ÂÉæÉ´ÊÉ∂Éß‰ª∂ÊÉÄ∑ÊÉº¨ÊÉµ∞: $(wc -l < phenotype_samples.tmp)"\
    echo "ÂÉ•±ÂÉ∞É¨ÊÉÄ∑ÊÉº¨ÊÉµ∞: $common"\
    \
    if [ $common -eq 0 ]; then\
        echo "ÈÉ¥ÉπËØØ: ÊÉÄ∑ÊÉº¨ID‰∏É≠ÂÉ¨πÈÉ•É≠"\
        echo "ÂÉø∫ÂÉªÉÄÂÉæÉ´ÊÉÄ∑ÊÉº¨Á§∫‰æÉ´:"\
        head -3 genotype_samples.tmp\
        echo "ËÉÅ®ÂÉæÉ´ÊÉÄ∑ÊÉº¨Á§∫‰æÉ´:"\
        head -3 phenotype_samples.tmp\
    fi\
    \
    rm -f genotype_samples.tmp phenotype_samples.tmp\
    exit 1\
fi\
\
echo "ËÉÅ®ÂÉæÉ´ÂÉ∞É®Âπ∂ÊÉ®É∞ÂÉ™Éø"\
\
# =============================================================================\
# Á¨¨ÂÉªÉªÊ≠•: ÊÉµ∞ÊÉ≠ÆË¥®ÈÉßÉØÊÉÆßÂÉ®∂\
# =============================================================================\
echo ""\
echo "Á¨¨ÂÉªÉªÊ≠•: ÊÉµ∞ÊÉ≠ÆË¥®ÈÉßÉØÊÉÆßÂÉ®∂..."\
\
# ËÆÉÅÁÆÉ∑ÂÉø∫ÊÉº¨ÁªÉøËÆÉÅ\
echo "4.1 ËÆÉÅÁÆÉ∑ÂÉø∫ÊÉº¨ÁªÉøËÆÉÅ..."\
plink --bfile data_with_pheno \\
      --freq \\
      --missing \\
      --hardy \\
      --allow-extra-chr \\
      --allow-no-sex \\
      --out qc_stats\
\
# Ë¥®ÊÉÆßËøÉßÊª§ÔºÉ®‰ΩøÁÉ¥®ÊÉÄÉßÂÉßÉ¶ÂÉØÇÊÉµ∞ÔºÉ©\
echo "4.2 Â∫É¥ÁÉ¥®Ë¥®ÊÉÆßËøÉßÊª§..."\
echo "Ë¥®ÊÉÆßÂÉØÇÊÉµ∞: --mind 0.05 --geno 0.05 --maf 0.01 --hwe 1e-6"\
\
plink --bfile data_with_pheno \\
      --mind 0.05 \\
      --geno 0.05 \\
      --maf 0.01 \\
      --hwe 1e-6 \\
      --make-bed \\
      --allow-extra-chr \\
      --allow-no-sex \\
      --out data_qc1\
\
if [ ! -f "data_qc1.bed" ]; then\
    echo "ÈÉ¥ÉπËØØ: Ë¥®ÈÉßÉØÊÉÆßÂÉ®∂Â§±Ë¥•"\
    exit 1\
fi\
\
echo "Ë¥®ÊÉÆßÂÆÉ¨ÊÉ®É∞"\
echo "Ë¥®ÊÉÆßÂÉ∞ÉÆÊÉÄ∑ÊÉº¨ÊÉµ∞: $(wc -l < data_qc1.fam)"\
echo "Ë¥®ÊÉÆßÂÉ∞ÉÆSNPÊÉµ∞: $(wc -l < data_qc1.bim)"\
\
# Ê£ÄÊÉø•ËÉÅ®ÂÉæÉ´ÂÉ®É¶Â∏É£\
echo "Ë¥®ÊÉÆßÂÉ∞ÉÆËÉÅ®ÂÉæÉ´ÂÉ®É¶Â∏É£:"\
echo "ÁÉ∑É•‰æÉ´ (ÊÉ§ÉøÁÉ∑É•): $(awk '$6==2' data_qc1.fam | wc -l)"\
echo "ÂØπÁÉ•ß (ÊÉ™É∑ÁÉ∑É•): $(awk '$6==1' data_qc1.fam | wc -l)"\
echo "Áº∫Â§±ËÉÅ®ÂÉæÉ´: $(awk '$6==-9 || $6==0' data_qc1.fam | wc -l)"\
\
# =============================================================================\
# Á¨¨‰∫É¥Ê≠•: Áæ§‰ΩÉ≥ÂÉ®É¶Â±ÇÊÉÆßÂÉ®∂\
# =============================================================================\
echo ""\
echo "Á¨¨‰∫É¥Ê≠•: Áæ§‰ΩÉ≥ÂÉ®É¶Â±ÇÊÉÆßÂÉ®∂..."\
\
# LDÂÉ©™ÊÉæÉΩ\
echo "5.1 LDÂÉ©™ÊÉæÉΩ (50 5 0.2)..."\
plink --bfile data_qc1 \\
      --indep-pairwise 50 5 0.2 \\
      --allow-extra-chr \\
      --allow-no-sex \\
      --out pruned_snps\
\
if [ -f "pruned_snps.prune.in" ]; then\
    pruned_count=$(wc -l < pruned_snps.prune.in)\
    echo "LDÂÉ©™ÊÉæÉΩÂÉ∞ÉÆ‰øÉΩÁÉµÉπ $pruned_count ‰∏™ÁÉ´¨Á´É´SNP"\
else\
    echo "Ë≠¶ÂÉ±É™: LDÂÉ©™ÊÉæÉΩÂ§±Ë¥•"\
fi\
\
# ‰∏ªÊÉ®É∞ÂÉ®É¶ÂÉ®É¶ÊÉæÉ∞\
echo "5.2 ‰∏ªÊÉ®É∞ÂÉ®É¶ÂÉ®É¶ÊÉæÉ∞..."\
if [ -f "pruned_snps.prune.in" ] && [ -s "pruned_snps.prune.in" ]; then\
    plink --bfile data_qc1 \\
          --extract pruned_snps.prune.in \\
          --pca 10 \\
          --allow-extra-chr \\
          --allow-no-sex \\
          --out pca_results\
    \
    if [ -f "pca_results.eigenvec" ]; then\
        echo "‰∏ªÊÉ®É∞ÂÉ®É¶ÂÉ®É¶ÊÉæÉ∞ÊÉ®É∞ÂÉ™ÉøÔºÉ¨‰∏∫ÊÉ∂Éß‰ª∂Ê∑ªÂÉ™ÉÄheader..."\
        echo "FID IID PC1 PC2 PC3 PC4 PC5 PC6 PC7 PC8 PC9 PC10" > pca_with_header.txt\
        cat pca_results.eigenvec >> pca_with_header.txt\
        PCA_SUCCESS=true\
    else\
        echo "‰∏ªÊÉ®É∞ÂÉ®É¶ÂÉ®É¶ÊÉæÉ∞Â§±Ë¥•"\
        PCA_SUCCESS=false\
    fi\
else\
    echo "Ë∑≥ËøÉß‰∏ªÊÉ®É∞ÂÉ®É¶ÂÉ®É¶ÊÉæÉ∞ÔºÉ®LDÂÉ©™ÊÉæÉΩÂ§±Ë¥•ÔºÉ©"\
    PCA_SUCCESS=false\
fi\
\
# =============================================================================\
# Á¨¨ÂÉ•≠Ê≠•: ÂÉ•≥ËÅÉ¥ÂÉ®É¶ÊÉæÉ∞\
# =============================================================================\
echo ""\
echo "Á¨¨ÂÉ•≠Ê≠•: ÂÉ•≥ËÅÉ¥ÂÉ®É¶ÊÉæÉ∞..."\
\
# ÂÉø∫ÊÉº¨ÂÉ®É¶ÊÉæÉ∞ÔºÉ®ÊÉ∑ÉÄÂÉ≠ÉØÂÉØÉ∏ÈÉßÉØÔºÉ©\
echo "6.1 ÂÉø∫ÊÉº¨logisticÂÉªÉæÂΩÉ≤ÂÉ®É¶ÊÉæÉ∞..."\
plink --bfile data_qc1 \\
      --logistic \\
      --ci 0.95 \\
      --allow-extra-chr \\
      --allow-no-sex \\
      --out gwas_basic\
\
# Ê£ÄÊÉø•ÂÉø∫ÊÉº¨ÂÉ®É¶ÊÉæÉ∞ÁªÉ≥ÊÉæÉº\
if [ -f "gwas_basic.assoc.logistic" ]; then\
    echo "ÂÉø∫ÊÉº¨ÂÉ®É¶ÊÉæÉ∞ÊÉ®É∞ÂÉ™Éø"\
    BASIC_SUCCESS=true\
else\
    echo "ÂÉø∫ÊÉº¨ÂÉ®É¶ÊÉæÉ∞Â§±Ë¥•"\
    BASIC_SUCCESS=false\
fi\
\
# ‰∏ªÊÉ®É∞ÂÉ®É¶ÊÉÄÉÅÊ≠£ÂÉ®É¶ÊÉæÉ∞\
if [ "$PCA_SUCCESS" = true ] && [ "$BASIC_SUCCESS" = true ]; then\
    echo "6.2 ‰∏ªÊÉ®É∞ÂÉ®É¶ÊÉÄÉÅÊ≠£logisticÂÉªÉæÂΩÉ≤ÂÉ®É¶ÊÉæÉ∞..."\
    plink --bfile data_qc1 \\
          --logistic \\
          --covar pca_with_header.txt \\
          --covar-name PC1,PC2,PC3,PC4,PC5 \\
          --ci 0.95 \\
          --allow-extra-chr \\
          --allow-no-sex \\
          --out gwas_adjusted\
    \
    if [ -f "gwas_adjusted.assoc.logistic" ]; then\
        echo "‰∏ªÊÉ®É∞ÂÉ®É¶ÊÉÄÉÅÊ≠£ÂÉ®É¶ÊÉæÉ∞ÊÉ®É∞ÂÉ™Éø"\
        MAIN_RESULT="gwas_adjusted"\
    else\
        echo "‰∏ªÊÉ®É∞ÂÉ®É¶ÊÉÄÉÅÊ≠£ÂÉ®É¶ÊÉæÉ∞Â§±Ë¥•ÔºÉ¨‰ΩøÁÉ¥®ÂÉø∫ÊÉº¨ÂÉ®É¶ÊÉæÉ∞ÁªÉ≥ÊÉæÉº"\
        MAIN_RESULT="gwas_basic"\
    fi\
else\
    echo "6.2 Ë∑≥ËøÉß‰∏ªÊÉ®É∞ÂÉ®É¶ÊÉÄÉÅÊ≠£ÂÉ®É¶ÊÉæÉ∞"\
    MAIN_RESULT="gwas_basic"\
fi\
\
# =============================================================================\
# Á¨¨‰∏É£Ê≠•: ÁªÉ≥ÊÉæÉºÂ§É§ÁÉ∞É¶ÂÉ≤É¨Á≠ÉªÈÄÉ©\
# =============================================================================\
echo ""\
echo "Á¨¨‰∏É£Ê≠•: ÁªÉ≥ÊÉæÉºÂ§É§ÁÉ∞É¶ÂÉ≤É¨Á≠ÉªÈÄÉ©..."\
\
RESULT_FILE="${MAIN_RESULT}.assoc.logistic"\
\
if [ ! -f "$RESULT_FILE" ]; then\
    echo "ÈÉ¥ÉπËØØ: ÂÉ•≥ËÅÉ¥ÂÉ®É¶ÊÉæÉ∞ÁªÉ≥ÊÉæÉºÊÉ∂Éß‰ª∂‰∏É≠Â≠É∏ÂÉº®: $RESULT_FILE"\
    exit 1\
fi\
\
echo "‰ΩøÁÉ¥®ÁªÉ≥ÊÉæÉºÊÉ∂Éß‰ª∂: $RESULT_FILE"\
\
# ÊÉ∏æÁ§∫ÊÉ∂Éß‰ª∂ÊÉÄºÂºÉØ\
echo ""\
echo "=== ÁªÉ≥ÊÉæÉºÊÉ∂Éß‰ª∂ÊÉÄºÂºÉØÊ£ÄÊÉø• ==="\
echo "HeaderËÉÅÉ¨:"\
head -1 "$RESULT_FILE"\
\
echo ""\
echo "ÂÉ®É∑‰øÉÅÊÅØ:"\
head -1 "$RESULT_FILE" | awk '{for(i=1; i<=NF; i++) printf "ÂÉ®É∑%d: %s\n", i, $i}'\
\
echo ""\
echo "ÊÄªÂÉ®É∑ÊÉµ∞: $(head -1 "$RESULT_FILE" | awk '{print NF}')"\
\
# ÊÉπ∫ËÉ£ΩÊ£ÄÊµÉ´PÂÄºÂÉ®É∑\
echo ""\
echo "=== ÊÉπ∫ËÉ£ΩÊ£ÄÊµÉ´PÂÄºÂÉ®É∑ ==="\
\
# ‰ΩøÁÉ¥®awkÁ≤æÁÉÅÆÊÉø•ÊÉ©æPÂÉ®É∑\
P_COL_NUM=$(head -1 "$RESULT_FILE" | awk '{\
    for(i=1; i<=NF; i++) {\
        if($i == "P") {\
            print i\
            exit\
        }\
    }\
}')\
\
if [ -z "$P_COL_NUM" ]; then\
    echo "ÊÉº™ÊÉ©æÂÉ®∞ÊÉÄÉßÂÉßÉ¶PÂÉ®É∑ÔºÉ¨Ê£ÄÊÉø•ÁÉª∏‰ººÂÉ®É∑ÂÉ∞É≠..."\
    head -1 "$RESULT_FILE" | awk '{\
        for(i=1; i<=NF; i++) {\
            if($i ~ /^P/ || $i ~ /P$/ || $i ~ /PVAL/ || $i ~ /P_VAL/) {\
                printf "ÂÉØØËÉ£ΩÁÉ∫É§PÂÄºÂÉ®É∑%d: %s\n", i, $i\
            }\
        }\
    }'\
    \
    # ÂÅÉßËÆæÊÉºÄÂÉ∞ÉÆ‰∏ÄÂÉ®É∑ÊÉ∏ØPÂÄº\
    P_COL_NUM=$(head -1 "$RESULT_FILE" | awk '{print NF}')\
    echo "ÂÅÉßËÆæÊÉºÄÂÉ∞ÉÆ‰∏ÄÂÉ®É∑(Á¨¨${P_COL_NUM}ÂÉ®É∑)ÊÉ∏ØPÂÄº"\
else\
    echo "ÊÉ©æÂÉ®∞PÂÄºÂÉ®É∑: Á¨¨${P_COL_NUM}ÂÉ®É∑"\
fi\
\
# È™É¨ËØÅPÂÄºÂÉ®É∑ÊÉµ∞ÊÉ≠Æ\
echo ""\
echo "=== È™É¨ËØÅPÂÄºÂÉ®É∑ÊÉµ∞ÊÉ≠Æ ==="\
echo "PÂÄºÊÉÄ∑ÊÉº¨ÔºÉ®ÂÉ©É≠10ËÉÅÉ¨ÔºÉ©:"\
awk -v col="$P_COL_NUM" 'NR>1 && NR<=11 {\
    if($col != "") printf "ËÉÅÉ¨%d: %s\n", NR, $col\
}' "$RESULT_FILE"\
\
# ÊÉØÉ∞ÂÉØÉ∂ADDÊ®ÉÅÂÉæÉ´ÁªÉ≥ÊÉæÉº\
echo ""\
echo "=== ÊÉØÉ∞ÂÉØÉ∂ADDÊ®ÉÅÂÉæÉ´ÁªÉ≥ÊÉæÉº ==="\
if grep -q "ADD" "$RESULT_FILE"; then\
    grep "ADD" "$RESULT_FILE" > gwas_results_ADD.txt\
    echo "ÊÉØÉ∞ÂÉØÉ∂ADDÊ®ÉÅÂÉæÉ´ÁªÉ≥ÊÉæÉº: $(wc -l < gwas_results_ADD.txt) ËÉÅÉ¨"\
else\
    echo "ÊÉº™ÊÉ©æÂÉ®∞ADDÊ®ÉÅÂÉæÉ´ÔºÉ¨‰ΩøÁÉ¥®ÊÉ©ÄÊÉºÉ©ÁªÉ≥ÊÉæÉº"\
    cp "$RESULT_FILE" gwas_results_ADD.txt\
fi\
\
# ÂÉº®ADDÁªÉ≥ÊÉæÉºÂÉ©É≠ÈÉΩÉÇÂÉ™ÉÄ‰∏É™header\
head -1 "$RESULT_FILE" > gwas_results_ADD_with_header.txt\
grep "ADD" "$RESULT_FILE" >> gwas_results_ADD_with_header.txt\
\
# ËÆÉÅÁÆÉ∑ÁªÉøËÆÉÅ‰øÉÅÊÅØÂÉ≤É¨ÈÉ∏É®ÂÄº\
SNP_COUNT=$(awk 'NR>1' gwas_results_ADD_with_header.txt | wc -l)\
echo ""\
echo "=== ÁªÉøËÆÉÅ‰øÉÅÊÅØ ==="\
echo "ADDÊ®ÉÅÂÉæÉ´SNPÊÉµ∞: $SNP_COUNT"\
\
# ËÆÉÅÁÆÉ∑BonferroniÈÉ∏É®ÂÄº\
if command -v python3 &> /dev/null; then\
    BONFERRONI=$(python3 -c "print(f'{0.05/$SNP_COUNT:.2e}')")\
elif command -v bc &> /dev/null; then\
    BONFERRONI=$(echo "scale=10; 0.05 / $SNP_COUNT" | bc -l | awk '{printf "%.2e", $1}')\
else\
    BONFERRONI=$(awk "BEGIN {printf \"%.2e\", 0.05/$SNP_COUNT}")\
fi\
\
echo "BonferroniÈÉ∏É®ÂÄº (0.05/SNPÊÉµ∞): $BONFERRONI"\
\
# ÂÆÉ∫‰πÉ©ÊÉ©ÄÊÉºÉ©ÈÉ∏É®ÂÄº\
GENOME_WIDE="5e-8"\
SUGGESTIVE="1e-5"\
NOMINAL="1e-4"\
EXPLORATORY="1e-3"\
\
echo ""\
echo "=== ‰ΩøÁÉ¥®ÁÉ∫É§ÈÉ∏É®ÂÄº ==="\
echo "BonferroniÊÉÄÉÅÊ≠£: $BONFERRONI"\
echo "ÂÉ•®ÂÉø∫ÂÉªÉÄÁªÉ§ÊÉ∏æËÉ±É∑: $GENOME_WIDE"\
echo "ÊÉØÉ∞Á§∫ÊÄßÂÉ•≥ËÅÉ¥: $SUGGESTIVE"\
echo "ÂÉ∞É≠‰πÉ©ÊÉ∏æËÉ±É∑: $NOMINAL"\
echo "ÊÉÆÉÇÁ¥ÉÇÊÄß: $EXPLORATORY"\
\
# Á≠ÉªÈÄÉ©‰∏É≠ÂÉ∞É¨ÈÉ∏É®ÂÄºÁÉ∫É§‰ΩÉ≠ÁÇπ\
echo ""\
echo "=== Á≠ÉªÈÄÉ©ÊÉ∏æËÉ±É∑‰ΩÉ≠ÁÇπ ==="\
\
# ÂÉßΩÊÉµ∞ÔºÉ∫Á≠ÉªÈÄÉ©‰ΩÉ≠ÁÇπ\
filter_snps() {\
    local input_file="$1"\
    local output_file="$2"\
    local threshold="$3"\
    local col_num="$4"\
    \
    awk -v col="$col_num" -v thresh="$threshold" '\
        NR==1 {print; next}\
        NR>1 && $col != "" && $col != "NA" {\
            p_val = $col + 0\
            thresh_val = thresh + 0\
            if (p_val < thresh_val) print\
        }\
    ' "$input_file" > "$output_file"\
}\
\
# ÊÉ©ßËÉÅÉ¨Á≠ÉªÈÄÉ©\
filter_snps "gwas_results_ADD_with_header.txt" "bonferroni_hits.txt" "$BONFERRONI" "$P_COL_NUM"\
filter_snps "gwas_results_ADD_with_header.txt" "genome_wide_hits.txt" "$GENOME_WIDE" "$P_COL_NUM"\
filter_snps "gwas_results_ADD_with_header.txt" "suggestive_hits.txt" "$SUGGESTIVE" "$P_COL_NUM"\
filter_snps "gwas_results_ADD_with_header.txt" "nominal_hits.txt" "$NOMINAL" "$P_COL_NUM"\
filter_snps "gwas_results_ADD_with_header.txt" "exploratory_hits.txt" "$EXPLORATORY" "$P_COL_NUM"\
\
# ÁªÉøËÆÉÅÁ≠ÉªÈÄÉ©ÁªÉ≥ÊÉæÉº\
echo ""\
echo "=== Á≠ÉªÈÄÉ©ÁªÉ≥ÊÉæÉºÁªÉøËÆÉÅ ==="\
echo "BonferroniÊÉ∏æËÉ±É∑ (P<$BONFERRONI): $(awk 'NR>1' bonferroni_hits.txt | wc -l) ‰ΩÉ≠ÁÇπ"\
echo "ÂÉ•®ÂÉø∫ÂÉªÉÄÁªÉ§ÊÉ∏æËÉ±É∑ (P<$GENOME_WIDE): $(awk 'NR>1' genome_wide_hits.txt | wc -l) ‰ΩÉ≠ÁÇπ"\
echo "ÊÉØÉ∞Á§∫ÊÄßÂÉ•≥ËÅÉ¥ (P<$SUGGESTIVE): $(awk 'NR>1' suggestive_hits.txt | wc -l) ‰ΩÉ≠ÁÇπ"\
echo "ÂÉ∞É≠‰πÉ©ÊÉ∏æËÉ±É∑ (P<$NOMINAL): $(awk 'NR>1' nominal_hits.txt | wc -l) ‰ΩÉ≠ÁÇπ"\
echo "ÊÉÆÉÇÁ¥ÉÇÊÄß (P<$EXPLORATORY): $(awk 'NR>1' exploratory_hits.txt | wc -l) ‰ΩÉ≠ÁÇπ"\
\
# ÊÉ∏æÁ§∫ÊÉºÄÊÉ∏æËÉ±É∑ÁÉ∫É§‰ΩÉ≠ÁÇπ\
echo ""\
echo "=== ÊÉºÄÊÉ∏æËÉ±É∑ÁÉ∫É§10‰∏™‰ΩÉ≠ÁÇπ ==="\
echo "PÂÄº            SNP              CHR    BP         OR     ‰ΩÉ≠ÁΩÆ"\
awk -v col="$P_COL_NUM" 'NR>1 && $col != "" && $col != "NA" {\
    p_val = $col + 0\
    or_val = ($7 != "" && $7 != "NA") ? $7 : "N/A"\
    printf "%.2e  %-15s  %-5s  %-8s  %-6s  %s:%s\n", p_val, $2, $1, $3, or_val, $1, $3\
}' gwas_results_ADD_with_header.txt | sort -k1,1g | head -10\
\
# =============================================================================\
# Á¨¨ÂÉ•´Ê≠•: ÁÉ¥ÉøÊÉ®É∞ÂÆÉ¨ÊÉµ¥ÊÉ™•ÂÉ±É™\
# =============================================================================\
echo ""\
echo "Á¨¨ÂÉ•´Ê≠•: ÁÉ¥ÉøÊÉ®É∞ÂÆÉ¨ÊÉµ¥ÊÉ™•ÂÉ±É™..."\
\
cat > final_analysis_report.txt << EOF\
=== ÂÆÉ¨ÊÉµ¥GWASÂÉ®É¶ÊÉæÉ∞ÊÉ™•ÂÉ±É™ ===\
ÂÉ®É¶ÊÉæÉ∞ÊÉ∑∂ÈÉ∑¥: $(date)\
ËÉ§É∫ÊÉº¨ÁÉ©É®ÊÉº¨: ÂÆÉ¨ÊÉµ¥ÁÉ©É®v1.0\
‰∏ªË¶ÅÂÉ®É¶ÊÉæÉ∞: $MAIN_RESULT\
PÂÄºÂÉ®É∑‰ΩÉ≠ÁΩÆ: Á¨¨${P_COL_NUM}ÂÉ®É∑\
\
=== ËæÉ≥ÂÉ••ÊÉµ∞ÊÉ≠ÆÁªÉøËÆÉÅ ===\
ÂÉÆÉøÂßÉ´VCFÊÉ∂Éß‰ª∂: final_filtered.recode.chr.vcf.gz\
ÂÉÆÉøÂßÉ´ËÉÅ®ÂÉæÉ´ÊÉ∂Éß‰ª∂: phe.txt\
ÂÉÆÉøÂßÉ´ÊÉÄ∑ÊÉº¨ÊÉµ∞: $(wc -l < raw_data.fam)\
ÂÉÆÉøÂßÉ´SNPÊÉµ∞: $(wc -l < raw_data.bim)\
\
=== Ë¥®ÊÉÆßÂÉ∞ÉÆÊÉµ∞ÊÉ≠ÆÁªÉøËÆÉÅ ===\
Ë¥®ÊÉÆßÂÉ∞ÉÆÊÉÄ∑ÊÉº¨ÊÉµ∞: $(wc -l < data_qc1.fam)\
Ë¥®ÊÉÆßÂÉ∞ÉÆSNPÊÉµ∞: $(wc -l < data_qc1.bim)\
ÊÉøÉ≥ËÉ©≤‰ΩÉ≥ÊÉµ∞: $(awk '{print $1}' data_qc1.bim | sort | uniq | wc -l)\
\
=== ËÉÅ®ÂÉæÉ´ÂÉ®É¶Â∏É£ ===\
ÁÉ∑É•‰æÉ´ÊÉµ∞ (ÊÉ§ÉøÁÉ∑É•): $(awk '$6==2' data_qc1.fam | wc -l)\
ÂØπÁÉ•ßÊÉµ∞ (ÊÉ™É∑ÁÉ∑É•): $(awk '$6==1' data_qc1.fam | wc -l)\
Áº∫Â§±ËÉÅ®ÂÉæÉ´: $(awk '$6==-9 || $6==0' data_qc1.fam | wc -l)\
\
=== Ë¥®ÊÉÆßÂÉØÇÊÉµ∞ ===\
‰∏™‰ΩÉ≥Áº∫Â§±ÁÉÆÉßÈÉ∏É®ÂÄº: 0.05\
SNPÁº∫Â§±ÁÉÆÉßÈÉ∏É®ÂÄº: 0.05\
ÊÉºÄÂ∞ÉØÁ≠É©‰ΩÉ≠ÂÉø∫ÂÉªÉÄÈÉÇÉ±ÁÉÆÉß: 0.01\
Hardy-Weinberg PÂÄºÈÉ∏É®ÂÄº: 1e-6\
LDÂÉ©™ÊÉæÉΩÂÉØÇÊÉµ∞: 50 5 0.2\
‰∏ªÊÉ®É∞ÂÉ®É¶ÊÉÄÉÅÊ≠£: $([ "$PCA_SUCCESS" = true ] && echo "ÊÉ∏Ø" || echo "ÂÉ∞¶")\
\
=== ÂÉ•≥ËÅÉ¥ÂÉ®É¶ÊÉæÉ∞ÁªÉ≥ÊÉæÉº ===\
BonferroniÊÉ∏æËÉ±É∑‰ΩÉ≠ÁÇπ (P<$BONFERRONI): $(awk 'NR>1' bonferroni_hits.txt | wc -l)\
ÂÉ•®ÂÉø∫ÂÉªÉÄÁªÉ§ÊÉ∏æËÉ±É∑‰ΩÉ≠ÁÇπ (P<$GENOME_WIDE): $(awk 'NR>1' genome_wide_hits.txt | wc -l)\
ÊÉØÉ∞Á§∫ÊÄßÂÉ•≥ËÅÉ¥‰ΩÉ≠ÁÇπ (P<$SUGGESTIVE): $(awk 'NR>1' suggestive_hits.txt | wc -l)\
ÂÉ∞É≠‰πÉ©ÊÉ∏æËÉ±É∑‰ΩÉ≠ÁÇπ (P<$NOMINAL): $(awk 'NR>1' nominal_hits.txt | wc -l)\
ÊÉÆÉÇÁ¥ÉÇÊÄß‰ΩÉ≠ÁÇπ (P<$EXPLORATORY): $(awk 'NR>1' exploratory_hits.txt | wc -l)\
\
=== ÁªÉ≥ÊÉæÉºËß£ÈÉßÉ™ ===\
- OR > 1: ËØ•Á≠É©‰ΩÉ≠ÂÉø∫ÂÉªÉÄÂÉÇÉæÂÉ™ÉÄÊÉ§ÉøÁÉ∑É•È£ÉÆÈÉπ©\
- OR < 1: ËØ•Á≠É©‰ΩÉ≠ÂÉø∫ÂÉªÉÄÂÉ•∑ÊÉºÉ©‰øÉΩÊÉ™§‰ΩÉºÁÉ¥®ÔºÉ®‰øÉ£ËøÉªÊÉ™É∑ÁÉ∑É•ÔºÉ©\
- BonferroniÊÉÄÉÅÊ≠£ÊÉ∏ØÊÉºÄ‰∏•ÊÉÄºÁÉ∫É§ÈÉ∏É®ÂÄº\
- ÊÉØÉ∞Á§∫ÊÄßÂÉ•≥ËÅÉ¥‰ΩÉ≠ÁÇπ(P<1e-5)ÂÄºÂæÉ∑ËøÉª‰∏ÄÊ≠•È™É¨ËØÅ\
- ÊÉ©ÄÊÉºÉ©ÊÉ∏æËÉ±É∑‰ΩÉ≠ÁÇπÈÉ£ΩÈÉºÄË¶ÅÂÉº®ÁÉ´¨Á´É´Áæ§‰ΩÉ≥‰∏≠È™É¨ËØÅ\
\
=== ÊÉºÄÊÉ∏æËÉ±É∑ÁÉ∫É§ÂÉ©É≠10‰∏™‰ΩÉ≠ÁÇπ ===\
EOF\
\
# Ê∑ªÂÉ™ÉÄÊÉºÄÊÉ∏æËÉ±É∑‰ΩÉ≠ÁÇπÁÉ∫É§ËØ¶ÁªÉ¶‰øÉÅÊÅØ\
echo "CHR           SNP              BP         A1   OR      PÂÄº        95%CI_L   95%CI_U" >> final_analysis_report.txt\
awk -v col="$P_COL_NUM" 'NR>1 && $col != "" && $col != "NA" {\
    p_val = $col + 0\
    or_val = ($7 != "" && $7 != "NA") ? $7 : "N/A"\
    l95 = ($9 != "" && $9 != "NA") ? $9 : "N/A"\
    u95 = ($10 != "" && $10 != "NA") ? $10 : "N/A"\
    printf "%-12s  %-15s  %-8s  %-3s  %-6s  %.2e  %-8s  %-8s\n", $1, $2, $3, $4, or_val, p_val, l95, u95\
}' gwas_results_ADD_with_header.txt | sort -k6,6g | head -10 >> final_analysis_report.txt\
\
# =============================================================================\
# Á¨¨‰πÉΩÊ≠•: ÂÉ®ÉªÂª∫ÂÉØØËßÉ¶ÂÉ¨É∂ËÉ§É∫ÊÉº¨\
# =============================================================================\
echo ""\
echo "Á¨¨‰πÉΩÊ≠•: ÂÉ®ÉªÂª∫ÂÉØØËßÉ¶ÂÉ¨É∂ËÉ§É∫ÊÉº¨..."\
\
cat > create_plots.R << 'EOF'\
#!/usr/bin/env Rscript\
\
# GWASÁªÉ≥ÊÉæÉºÂÉØØËßÉ¶ÂÉ¨É∂ËÉ§É∫ÊÉº¨\
cat("ÂºÄÂßÉ´GWASÁªÉ≥ÊÉæÉºÂÉØØËßÉ¶ÂÉ¨É∂...\n")\
\
# Ê£ÄÊÉø•Âπ∂ÂÆÉ©Ë£É•ÂøÉ•Ë¶ÅÁÉ∫É§ÂÉ¨É•\
packages <- c("qqman", "data.table")\
for (pkg in packages) {\
    if (!require(pkg, character.only = TRUE, quietly = TRUE)) {\
        cat("ÂÆÉ©Ë£É•ÂÉ¨É•:", pkg, "\n")\
        install.packages(pkg, repos = "https://cran.rstudio.com/")\
        library(pkg, character.only = TRUE)\
    }\
}\
\
# ËØªÂÉØÉ∂GWASÁªÉ≥ÊÉæÉº\
if (!file.exists("gwas_results_ADD_with_header.txt")) {\
    stop("ÈÉ¥ÉπËØØ: ÊÉ©æ‰∏É≠ÂÉ®∞gwas_results_ADD_with_header.txtÊÉ∂Éß‰ª∂")\
}\
\
cat("ËØªÂÉØÉ∂GWASÁªÉ≥ÊÉæÉºÊÉ∂Éß‰ª∂...\n")\
gwas <- fread("gwas_results_ADD_with_header.txt", data.table = FALSE)\
\
cat("ÊÉµ∞ÊÉ≠ÆÁª¥Â∫¶:", nrow(gwas), "ËÉÅÉ¨", ncol(gwas), "ÂÉ®É∑\n")\
cat("ÂÉ®É∑ÂÉ∞É≠:", colnames(gwas), "\n")\
\
# ÊÉπ∫ËÉ£ΩËØÉ¶ÂÉ®´ÂÉ•≥ÈÉ¥ÆÂÉ®É∑\
# PÂÄºÂÉ®É∑\
p_col <- which(colnames(gwas) == "P")\
if (length(p_col) == 0) {\
    p_candidates <- grep("P", colnames(gwas), ignore.case = TRUE)\
    if (length(p_candidates) > 0) {\
        p_col <- p_candidates[1]\
        cat("‰ΩøÁÉ¥®PÂÄºÂÉ®É∑:", colnames(gwas)[p_col], "\n")\
    } else {\
        p_col <- ncol(gwas)  # ÂÅÉßËÆæÊÉºÄÂÉ∞ÉÆ‰∏ÄÂÉ®É∑\
        cat("ÂÅÉßËÆæÊÉºÄÂÉ∞ÉÆ‰∏ÄÂÉ®É∑ÊÉ∏ØPÂÄº\n")\
    }\
}\
\
# ÊÉØÉ∞ÂÉØÉ∂PÂÄº\
gwas$P_value <- as.numeric(gwas[, p_col])\
\
# Â§É§ÁÉ∞É¶ÊÉøÉ≥ËÉ©≤‰ΩÉ≥ÁºÉ∂ÂÉØ∑\
if ("CHR" %in% colnames(gwas)) {\
    gwas$CHR_original <- gwas$CHR\
    \
    # Â§É§ÁÉ∞É¶OVÂºÄÂ§¥ÁÉ∫É§ÊÉøÉ≥ËÉ©≤‰ΩÉ≥ÁºÉ∂ÂÉØ∑\
    gwas$CHR_numeric <- as.numeric(gsub("OV0?", "", gwas$CHR))\
    \
    # Â¶ÇÊÉæÉºËΩ¨ÊÉ≠ÉÇÂ§±Ë¥•ÔºÉ¨‰ΩøÁÉ¥®ÊÉ∏ÉÄÂ∞É§\
    if (any(is.na(gwas$CHR_numeric))) {\
        unique_chrs <- unique(gwas$CHR)\
        chr_mapping <- setNames(1:length(unique_chrs), unique_chrs)\
        gwas$CHR_numeric <- chr_mapping[gwas$CHR]\
        cat("ÊÉøÉ≥ËÉ©≤‰ΩÉ≥ÊÉ∏ÉÄÂ∞É§:\n")\
        print(data.frame(Original = names(chr_mapping), Numeric = chr_mapping))\
    }\
} else {\
    stop("ÈÉ¥ÉπËØØ: ÊÉ©æ‰∏É≠ÂÉ®∞CHRÂÉ®É∑")\
}\
\
# ÂÉßÉ¶Â§ÉßÁªÉ∏ÂÉªæÊÉµ∞ÊÉ≠Æ\
plot_data <- data.frame(\
    SNP = gwas$SNP,\
    CHR = gwas$CHR_numeric,\
    BP = as.numeric(gwas$BP),\
    P = gwas$P_value\
)\
\
# ÁßªÈÉπ§ÊÉ∑ÉÄÊÉµÉ®ÊÉµ∞ÊÉ≠Æ\
valid_rows <- !is.na(plot_data$P) & !is.na(plot_data$BP) & \
              plot_data$P > 0 & plot_data$P <= 1\
plot_data <- plot_data[valid_rows, ]\
\
cat("ÊÉºÉ©ÊÉµÉ®ÊÉµ∞ÊÉ≠ÆÁÇπÊÉµ∞:", nrow(plot_data), "\n")\
cat("PÂÄºËÉ¨É£ÂÉª¥: [", min(plot_data$P), ",", max(plot_data$P), "]\n")\
\
# ÁÉ¥ÉøÊÉ®É∞ManhattanÂÉªæ\
cat("ÁÉ¥ÉøÊÉ®É∞ManhattanÂÉªæ...\n")\
png("manhattan_plot.png", width = 1400, height = 700, res = 100)\
par(mar = c(5, 5, 4, 2))\
\
tryCatch({\
    manhattan(plot_data, \
              main = "GWAS Manhattan Plot (Disease Resistance)",\
              genomewideline = -log10(5e-8),\
              suggestiveline = -log10(1e-5),\
              col = c("navy", "orange"),\
              cex = 0.6,\
              cex.axis = 0.8,\
              ylim = c(0, max(-log10(plot_data$P)) * 1.1))\
}, error = function(e) {\
    plot(1, type = "n", main = "Manhattan PlotÁÉ¥ÉøÊÉ®É∞Â§±Ë¥•", \
         xlab = "Position", ylab = "-log10(P)")\
    text(1, 1, paste("ÈÉ¥ÉπËØØ:", e$message), cex = 0.8)\
})\
dev.off()\
\
# ÁÉ¥ÉøÊÉ®É∞QQÂÉªæ\
cat("ÁÉ¥ÉøÊÉ®É∞QQÂÉªæ...\n")\
png("qq_plot.png", width = 700, height = 700, res = 100)\
par(mar = c(5, 5, 4, 2))\
\
tryCatch({\
    qq(plot_data$P, main = "QQ Plot")\
}, error = function(e) {\
    plot(1, type = "n", main = "QQ PlotÁÉ¥ÉøÊÉ®É∞Â§±Ë¥•",\
         xlab = "Expected -log10(P)", ylab = "Observed -log10(P)")\
    text(1, 1, paste("ÈÉ¥ÉπËØØ:", e$message), cex = 0.8)\
})\
dev.off()\
\
# ÁÉ¥ÉøÊÉ®É∞PÂÄºÂÉ®É¶Â∏É£ÁÉª¥ÊÉ∂πÂÉªæ\
cat("ÁÉ¥ÉøÊÉ®É∞PÂÄºÂÉ®É¶Â∏É£ÂÉªæ...\n")\
png("p_value_distribution.png", width = 800, height = 600, res = 100)\
par(mar = c(5, 5, 4, 2))\
hist(plot_data$P, breaks = 50, main = "PÂÄºÂÉ®É¶Â∏É£", \
     xlab = "PÂÄº", ylab = "ÈÉÇÉ±ÊÉµ∞", col = "lightblue")\
abline(v = c(5e-8, 1e-5, 1e-4, 1e-3), col = c("red", "orange", "green", "blue"), lty = 2)\
legend("topright", c("5e-8", "1e-5", "1e-4", "1e-3"), \
       col = c("red", "orange", "green", "blue"), lty = 2, cex = 0.8)\
dev.off()\
\
# ‰øÉΩÂ≠É∏ÊÉøÉ≥ËÉ©≤‰ΩÉ≥ÊÉ∏ÉÄÂ∞É§\
if (exists("chr_mapping")) {\
    write.table(data.frame(Original_CHR = names(chr_mapping), \
                          Numeric_CHR = chr_mapping),\
                "chromosome_mapping.txt", \
                row.names = FALSE, quote = FALSE, sep = "\t")\
}\
\
# ÁÉ¥ÉøÊÉ®É∞ÁÆÄÂÉ≠ÉµÁªÉøËÆÉÅ\
stats <- data.frame(\
    Threshold = c("5e-8", "1e-5", "1e-4", "1e-3"),\
    Count = c(\
        sum(plot_data$P < 5e-8),\
        sum(plot_data$P < 1e-5),\
        sum(plot_data$P < 1e-4),\
        sum(plot_data$P < 1e-3)\
    )\
)\
\
write.table(stats, "p_value_statistics.txt", \
            row.names = FALSE, quote = FALSE, sep = "\t")\
\
cat("\n=== ÂÉØØËßÉ¶ÂÉ¨É∂ÂÆÉ¨ÊÉ®É∞ ===\n")\
cat("ÁÉ¥ÉøÊÉ®É∞ÁÉ∫É§ÊÉ∂Éß‰ª∂:\n")\
cat("- manhattan_plot.png\n")\
cat("- qq_plot.png\n") \
cat("- p_value_distribution.png\n")\
cat("- p_value_statistics.txt\n")\
if (exists("chr_mapping")) cat("- chromosome_mapping.txt\n")\
\
cat("\n=== ÁªÉøËÆÉÅÊÉ±É∏Ë¶Å ===\n")\
cat("ÊÄªSNPÊÉµ∞:", nrow(plot_data), "\n")\
cat("ÊÉøÉ≥ËÉ©≤‰ΩÉ≥ÊÉµ∞:", length(unique(plot_data$CHR)), "\n")\
cat("ÊÉºÄÂ∞ÉØPÂÄº:", min(plot_data$P), "\n")\
print(stats)\
EOF\
\
# =============================================================================\
# ÂÆÉ¨ÊÉ®É∞ÊÉ™•ÂÉ±É™\
# =============================================================================\
echo ""\
echo "================================================"\
echo "ÂÆÉ¨ÊÉµ¥GWASÂÉ®É¶ÊÉæÉ∞ÂÆÉ¨ÊÉ®É∞ÔºÅ"\
echo "================================================"\
\
# ÊÉ∏æÁ§∫‰∏ªË¶ÅÁªÉ≥ÊÉæÉºÊÉ±É∏Ë¶Å\
echo ""\
echo "=== ÂÉ®É¶ÊÉæÉ∞ÊÉ±É∏Ë¶Å ==="\
echo "‰∏ªË¶ÅÂÉ®É¶ÊÉæÉ∞ÊÉ∂πÊ≥Éµ: $MAIN_RESULT"\
echo "PÂÄºÂÉ®É∑‰ΩÉ≠ÁΩÆ: Á¨¨${P_COL_NUM}ÂÉ®É∑"\
echo "Ë¥®ÊÉÆßÂÉ∞ÉÆÊÉÄ∑ÊÉº¨ÊÉµ∞: $(wc -l < data_qc1.fam)"\
echo "Ë¥®ÊÉÆßÂÉ∞ÉÆSNPÊÉµ∞: $(wc -l < data_qc1.bim)"\
echo "ÁÉ∑É•‰æÉ´/ÂØπÁÉ•ßÊØÉ¥‰æÉ´: $(awk '$6==2' data_qc1.fam | wc -l)/$(awk '$6==1' data_qc1.fam | wc -l)"\
\
echo ""\
echo "=== ÂÉ•≥ËÅÉ¥ÁªÉ≥ÊÉæÉº ==="\
echo "BonferroniÊÉ∏æËÉ±É∑ (P<$BONFERRONI): $(awk 'NR>1' bonferroni_hits.txt | wc -l) ‰ΩÉ≠ÁÇπ"\
echo "ÂÉ•®ÂÉø∫ÂÉªÉÄÁªÉ§ÊÉ∏æËÉ±É∑ (P<$GENOME_WIDE): $(awk 'NR>1' genome_wide_hits.txt | wc -l) ‰ΩÉ≠ÁÇπ"\
echo "ÊÉØÉ∞Á§∫ÊÄßÂÉ•≥ËÅÉ¥ (P<$SUGGESTIVE): $(awk 'NR>1' suggestive_hits.txt | wc -l) ‰ΩÉ≠ÁÇπ"\
echo "ÂÉ∞É≠‰πÉ©ÊÉ∏æËÉ±É∑ (P<$NOMINAL): $(awk 'NR>1' nominal_hits.txt | wc -l) ‰ΩÉ≠ÁÇπ"\
\
echo ""\
echo "=== ËæÉ≥ÂÉß∫ÊÉ∂Éß‰ª∂ ==="\
echo "‰∏ªË¶ÅÁªÉ≥ÊÉæÉºÊÉ∂Éß‰ª∂:"\
echo "‚ÉºÉ≥ final_analysis_report.txt - ÂÆÉ¨ÊÉµ¥ÂÉ®É¶ÊÉæÉ∞ÊÉ™•ÂÉ±É™"\
echo "‚ÉºÉ≥ gwas_results_ADD_with_header.txt - ‰∏ªË¶ÅGWASÁªÉ≥ÊÉæÉº"\
\
